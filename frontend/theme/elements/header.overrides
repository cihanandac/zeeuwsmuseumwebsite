div.logo {
  display: flex;
  height: 100%;
  align-items: center;
  align-items: center;
}

#logo-written.logo-written {
  height: 57px;
  padding: 22px 15px 42px;
  color: #7bb0e7 !important;
  font-family: 'UniversCn', Arial, sans-serif;
  font-size: 30px;
  -webkit-font-smoothing: antialiased;
  line-height: 20px;
  text-transform: uppercase;
  width: auto !important;
}

#writtenlogo {
  padding-left: 0px;
}

.ui.basic.segment .header .logo-nav-wrapper,
.tools-search-wrapper {
  height: 30px;
  justify-content: space-between;
  // padding-top: 22px;
  // padding-bottom: 20px;
}

#header-wrapper {
  width: 100%;
}

.header {
  flex-wrap: nowrap !important;
}

.tools-search-wrapper {
  max-width: 350px;
  align-self: center;
}

.navigation-language-wrapper {
  display: flex;
  flex-wrap: nowrap;
  // flex: 1 1 auto;
  // justify-content: flex-end;
}

.ui.pointing.secondary {
  align-content: space-around;
}

div.ui.basic.segment.header-wrapper {
  position: fixed !important;
  z-index: 998 !important;
  top: 0;
  padding: 0px !important;
  margin-bottom: 0px;
  background-color: #ffffff;
  transition-duration: 300ms;
  transition-property: all;
  transition-timing-function: cubic-bezier(0.4, 0, 0.2, 1);

  /* height: 60px; */

  .ui.container.header-container {
    z-index: 99999 !important;
    width: 100% !important;
    height: 60px;
    padding-top: 14px;
    padding-right: 0px;
    padding-bottom: 14px;
    padding-left: 9px;
    border-bottom: 1px solid #e7e7e7;
    margin: 0% !important;
    background-color: #ffffff;
  }

  .logo {
    height: 25px !important;
    margin-right: 0px;
  }
}

