//   z-index: 9999;
// }

div.ui.basic.segment.header-wrapper.hide {
  top: -560px;
}

div.ui.pointing.secondary.stackable {
  margin: 0 !important;
  font-family: 'UniversCn', Arial, sans-serif;
  font-size: 17px;
  letter-spacing: 0.1em;
}

.mobile-menu-nav, .mobile-menu.mobile-menu-enter-done, .mobile-menu.mobile-menu-enter-active {
  padding-top: 0px !important;
}
.mobile-menu-nav > .ui.pointing.secondary.stackable.menu{
  flex-direction: column;
  text-align: left !important;
  width: 100%;
  align-items: flex-end !important;

  .item.simple, .tools-search-wrapper{
    align-self: flex-start;
    justify-content: flex-start !important;
    // width: 100%;
  }
  .search-tool{
    width: 100%;
    border-bottom: 0.5px solid #e7e7e7 !important;
    display: flex;
    order: -1;
    // flex-direction: column-reverse;
    justify-content: flex-start;
    padding-bottom: 0px;
  }
  .tools-search-wrapper{
    width: 100%;
    padding-bottom: 0px;
    // align-self: flex-start;
    .search{
      align-self: flex-start;
      padding-bottom: 0px;
    }
    .language-selector-wrapper, .ui.form{
      border-left: none;
      align-self: flex-start;
    }
    .ui.form{
      .field.searchbox > button{
        margin-left: 0px;
        svg{
          padding-bottom: 0% !important;
        }
      }
    }
  }
}

div.menu.transition {
  right: 0% !important;
  left: auto !important;
  min-width: 238px !important;
  padding: 22px 0px 18px !important;
  font-family: 'UniversLight', Arial, sans-serif;
  border-radius: 0% !important;

  div.item {
    padding-bottom: 0% !important;
    margin-bottom: 0% !important;
  }
}
div.menu.transition.dropdownContentPage, div.menu.transition.item.simple.language-selector_dropdown{
  left: auto !important;
}

div.menu.transition {
  border-top: 1px solid #7bb0e7 !important;
}

nav#navigation.navigation {
  height: 60px;

  div,
  a {
    color: #000000 !important;

    :hover {
      color: #7bb0e7 !important;
    }
  }
}

div#inhoudDropdown {
